<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot_coolshark.mapper.UserMapper">
    <select id="selectByUsername" resultType="com.example.springboot_coolshark.pojo.vo.UserVO">
        SELECT id,password,nickname FROM user WHERE username=#{username}
    </select>


</mapper>
